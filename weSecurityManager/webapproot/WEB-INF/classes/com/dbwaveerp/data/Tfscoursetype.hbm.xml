<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tfscoursetype" table="tfscoursetype" catalog="dbwaveerp" dynamic-insert="false" dynamic-update="false">
        <id name="seqid" type="integer">
            <column name="seqid"/>
            <generator class="identity"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20"/>
        </property>
        <property name="typeid" type="string">
            <column name="typeid" length="20"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="65535"/>
        </property>
        <property name="startdate" type="date">
            <column name="startdate" length="10"/>
        </property>
        <property name="enddate" type="date">
            <column name="enddate" length="10"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>