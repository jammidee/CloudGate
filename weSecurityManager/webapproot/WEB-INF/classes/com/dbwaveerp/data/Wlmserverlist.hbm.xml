<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Wlmserverlist" table="wlmserverlist" dynamic-insert="false" dynamic-update="false">
        <id name="juid" type="string">
            <column name="juid" length="36"/>
            <generator class="assigned"/>
        </id>
        <property name="farmid" type="string">
            <column name="farmid" length="20"/>
        </property>
        <property name="serverid" type="string">
            <column name="serverid" length="20"/>
        </property>
        <property name="servername" type="string">
            <column name="servername" length="200"/>
        </property>
        <property name="conusers" type="integer">
            <column name="conusers"/>
        </property>
        <property name="strictlimit" type="string">
            <column name="strictlimit" length="5"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
        <property name="deleted" type="integer">
            <column name="deleted"/>
        </property>
    </class>
</hibernate-mapping>