<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tfsroomreserve" table="tfsroomreserve" catalog="dbwaveerp" dynamic-insert="false" dynamic-update="false">
        <id name="seqid" type="integer">
            <column name="seqid"/>
            <generator class="identity"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20"/>
        </property>
        <property name="reserveid" type="string">
            <column name="reserveid" length="20"/>
        </property>
        <property name="reservetype" type="string">
            <column name="reservetype" length="20"/>
        </property>
        <property name="reservedesc" type="string">
            <column name="reservedesc" length="150"/>
        </property>
        <property name="classid" type="string">
            <column name="classid" length="20"/>
        </property>
        <property name="roomid" type="string">
            <column name="roomid" length="20"/>
        </property>
        <property name="notes" type="string">
            <column name="notes" length="65535"/>
        </property>
        <property name="startdate" type="date">
            <column name="startdate" length="10"/>
        </property>
        <property name="enddate" type="date">
            <column name="enddate" length="10"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>