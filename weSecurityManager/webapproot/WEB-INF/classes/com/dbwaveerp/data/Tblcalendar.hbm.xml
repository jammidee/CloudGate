<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tblcalendar" table="tblcalendar" dynamic-insert="false" dynamic-update="false">
        <id name="juid" type="string">
            <column name="juid" length="36"/>
            <generator class="assigned"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20" not-null="true"/>
        </property>
        <property name="caldate" type="date">
            <column name="caldate" length="10"/>
        </property>
        <property name="userid" type="string">
            <column name="userid" length="20" not-null="true"/>
        </property>
        <property name="groupid" type="string">
            <column name="groupid" length="20"/>
        </property>
        <property name="subject" type="string">
            <column name="subject" length="200"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="65535"/>
        </property>
        <property name="priority" type="string">
            <column name="priority" length="5"/>
        </property>
        <property name="startdate" type="date">
            <column name="startdate" length="10"/>
        </property>
        <property name="enddate" type="date">
            <column name="enddate" length="10"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>