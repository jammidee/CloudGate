<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tblorgdivision" table="tblorgdivision" dynamic-insert="false" dynamic-update="false">
        <id name="juid" type="string">
            <column name="juid" length="36"/>
            <generator class="assigned"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20"/>
        </property>
        <property name="orgid" type="string">
            <column name="orgid" length="20"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="200"/>
        </property>
        <property name="pid" type="string">
            <column name="pid" length="36"/>
        </property>
        <property name="contact01" type="string">
            <column name="contact01" length="20"/>
        </property>
        <property name="contact02" type="string">
            <column name="contact02" length="20"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>