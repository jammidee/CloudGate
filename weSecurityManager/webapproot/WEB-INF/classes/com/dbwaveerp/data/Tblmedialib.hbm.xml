<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tblmedialib" table="tblmedialib" dynamic-insert="false" dynamic-update="false">
        <id name="seqid" type="integer">
            <column name="seqid"/>
            <generator class="identity"/>
        </id>
        <property name="juid" type="string">
            <column name="juid" length="36" not-null="true"/>
        </property>
        <property name="entity" type="string">
            <column name="entity" length="50"/>
        </property>
        <property name="appid" type="string">
            <column name="appid" length="50"/>
        </property>
        <property name="userid" type="string">
            <column name="userid" length="50"/>
        </property>
        <property name="filedata" type="binary">
            <column name="filedata"/>
        </property>
        <property name="filename" type="string">
            <column name="filename" length="100"/>
        </property>
        <property name="filext" type="string">
            <column name="filext" length="10"/>
        </property>
        <property name="filesize" type="string">
            <column name="filesize" length="16777215"/>
        </property>
        <property name="filedate" type="date">
            <column name="filedate" length="10"/>
        </property>
        <property name="filesrc" type="string">
            <column name="filesrc" length="5"/>
        </property>
        <property name="ispicture" type="integer">
            <column name="ispicture"/>
        </property>
        <property name="ispdf" type="integer">
            <column name="ispdf"/>
        </property>
        <property name="hasviewer" type="integer">
            <column name="hasviewer"/>
        </property>
        <property name="isreport" type="integer">
            <column name="isreport"/>
        </property>
        <property name="reporttitle" type="string">
            <column name="reporttitle" length="200"/>
        </property>
        <property name="reportpara" type="string">
            <column name="reportpara" length="65535"/>
        </property>
        <property name="reportgrp" type="string">
            <column name="reportgrp" length="20"/>
        </property>
        <property name="connprofile" type="string">
            <column name="connprofile" length="20"/>
        </property>
        <property name="reportparent" type="string">
            <column name="reportparent" length="20"/>
        </property>
        <property name="geodesc" type="string">
            <column name="geodesc" length="200"/>
        </property>
        <property name="geolon" type="double">
            <column name="geolon" precision="22"/>
        </property>
        <property name="geolat" type="double">
            <column name="geolat" precision="22"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>