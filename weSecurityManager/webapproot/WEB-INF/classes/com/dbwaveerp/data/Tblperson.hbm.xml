<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tblperson" table="tblperson" dynamic-insert="false" dynamic-update="false">
        <id name="juid" type="string">
            <column name="juid" length="36"/>
            <generator class="assigned"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20" not-null="true"/>
        </property>
        <property name="partyid" type="string">
            <column name="partyid" length="20" not-null="true"/>
        </property>
        <property name="title" type="string">
            <column name="title" length="20"/>
        </property>
        <property name="firstname" type="string">
            <column name="firstname" length="100" not-null="true"/>
        </property>
        <property name="middlename" type="string">
            <column name="middlename" length="100"/>
        </property>
        <property name="lastname" type="string">
            <column name="lastname" length="100" not-null="true"/>
        </property>
        <property name="suffix" type="string">
            <column name="suffix" length="20"/>
        </property>
        <property name="addr01" type="string">
            <column name="addr01" length="65535"/>
        </property>
        <property name="addr02" type="string">
            <column name="addr02" length="65535"/>
        </property>
        <property name="city" type="string">
            <column name="city" length="100"/>
        </property>
        <property name="empstate" type="string">
            <column name="empstate" length="100"/>
        </property>
        <property name="zipcode" type="string">
            <column name="zipcode" length="20"/>
        </property>
        <property name="country" type="string">
            <column name="country" length="50"/>
        </property>
        <property name="email" type="string">
            <column name="email" length="200" not-null="true"/>
        </property>
        <property name="countrycodehome" type="string">
            <column name="countrycodehome" length="5"/>
        </property>
        <property name="areacodehome" type="string">
            <column name="areacodehome" length="5"/>
        </property>
        <property name="phonehome" type="string">
            <column name="phonehome" length="20"/>
        </property>
        <property name="extensionhome" type="string">
            <column name="extensionhome" length="10"/>
        </property>
        <property name="countrycodework" type="string">
            <column name="countrycodework" length="5"/>
        </property>
        <property name="areacodework" type="string">
            <column name="areacodework" length="5"/>
        </property>
        <property name="phonework" type="string">
            <column name="phonework" length="20"/>
        </property>
        <property name="extensionwork" type="string">
            <column name="extensionwork" length="10"/>
        </property>
        <property name="countrycodefax" type="string">
            <column name="countrycodefax" length="5"/>
        </property>
        <property name="areacodefax" type="string">
            <column name="areacodefax" length="5"/>
        </property>
        <property name="phonefax" type="string">
            <column name="phonefax" length="20"/>
        </property>
        <property name="extensionfax" type="string">
            <column name="extensionfax" length="10"/>
        </property>
        <property name="countrycodemobile" type="string">
            <column name="countrycodemobile" length="5"/>
        </property>
        <property name="areacodemobile" type="string">
            <column name="areacodemobile" length="5"/>
        </property>
        <property name="phonemobile" type="string">
            <column name="phonemobile" length="20"/>
        </property>
        <property name="extensionmobile" type="string">
            <column name="extensionmobile" length="10"/>
        </property>
        <property name="roletype" type="string">
            <column name="roletype" length="20"/>
        </property>
        <property name="userjuid" type="string">
            <column name="userjuid" length="30"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20" not-null="true"/>
        </property>
        <property name="pid" type="string">
            <column name="pid" length="20"/>
        </property>
        <property name="deleted" type="integer">
            <column name="deleted"/>
        </property>
    </class>
</hibernate-mapping>