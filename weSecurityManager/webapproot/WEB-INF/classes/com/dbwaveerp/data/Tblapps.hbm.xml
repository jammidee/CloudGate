<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tblapps" table="tblapps" dynamic-insert="false" dynamic-update="false">
        <id name="seqid" type="integer">
            <column name="seqid"/>
            <generator class="identity"/>
        </id>
        <property name="juid" type="string">
            <column name="juid" length="36" not-null="true"/>
        </property>
        <property name="appid" type="string">
            <column name="appid" length="20" not-null="true"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="65535"/>
        </property>
    </class>
</hibernate-mapping>