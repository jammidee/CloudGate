<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Wdmfolders" table="wdmfolders" dynamic-insert="false" dynamic-update="false">
        <id name="juid" type="string">
            <column name="juid" length="36"/>
            <generator class="assigned"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20"/>
        </property>
        <property name="appid" type="string">
            <column name="appid" length="50"/>
        </property>
        <property name="siteid" type="string">
            <column name="siteid" length="20"/>
        </property>
        <property name="folderid" type="string">
            <column name="folderid" length="20"/>
        </property>
        <property name="foldername" type="string">
            <column name="foldername" length="100"/>
        </property>
        <property name="folderdesc" type="string">
            <column name="folderdesc" length="200"/>
        </property>
        <property name="email" type="string">
            <column name="email" length="200"/>
        </property>
        <property name="startdate" type="date">
            <column name="startdate" length="10"/>
        </property>
        <property name="enddate" type="date">
            <column name="enddate" length="10"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
        <property name="pid" type="string">
            <column name="pid" length="36" not-null="true"/>
        </property>
        <property name="deleted" type="integer">
            <column name="deleted"/>
        </property>
        <property name="seqid" type="integer">
            <column name="seqid"/>
        </property>
    </class>
</hibernate-mapping>