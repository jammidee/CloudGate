<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Wdmcategories" table="wdmcategories" dynamic-insert="false" dynamic-update="false">
        <id name="juid" type="string">
            <column name="juid" length="36"/>
            <generator class="assigned"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20"/>
        </property>
        <property name="appid" type="string">
            <column name="appid" length="20"/>
        </property>
        <property name="siteid" type="string">
            <column name="siteid" length="20"/>
        </property>
        <property name="catid" type="string">
            <column name="catid" length="100"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="200"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
        <property name="pid" type="string">
            <column name="pid" length="36" not-null="true"/>
        </property>
        <property name="deleted" type="integer">
            <column name="deleted"/>
        </property>
    </class>
</hibernate-mapping>