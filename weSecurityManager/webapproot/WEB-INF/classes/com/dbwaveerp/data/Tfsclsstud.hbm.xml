<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tfsclsstud" table="tfsclsstud" catalog="dbwaveerp" dynamic-insert="false" dynamic-update="false">
        <id name="seqid" type="integer">
            <column name="seqid"/>
            <generator class="identity"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20"/>
        </property>
        <property name="classid" type="string">
            <column name="classid" length="20"/>
        </property>
        <property name="studid" type="string">
            <column name="studid" length="20"/>
        </property>
        <property name="fname" type="string">
            <column name="fname" length="20"/>
        </property>
        <property name="mname" type="string">
            <column name="mname" length="20"/>
        </property>
        <property name="lname" type="string">
            <column name="lname" length="20"/>
        </property>
        <property name="grade" type="float">
            <column name="grade" precision="12"/>
        </property>
        <property name="passed" type="string">
            <column name="passed" length="20"/>
        </property>
        <property name="userid" type="string">
            <column name="userid" length="20"/>
        </property>
        <property name="datecomplete" type="date">
            <column name="datecomplete" length="10"/>
        </property>
        <property name="approved" type="string">
            <column name="approved" length="20"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>