<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tbladmapping" table="tbladmapping" dynamic-insert="false" dynamic-update="false">
        <id name="juid" type="string">
            <column name="juid" length="36"/>
            <generator class="assigned"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20"/>
        </property>
        <property name="urlid" type="string">
            <column name="urlid" length="200"/>
        </property>
        <property name="userid" type="string">
            <column name="userid" length="50"/>
        </property>
        <property name="userpass" type="string">
            <column name="userpass" length="50"/>
        </property>
        <property name="aduserid" type="string">
            <column name="aduserid" length="50"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>