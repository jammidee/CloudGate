<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tfsclass" table="tfsclass" catalog="dbwaveerp" dynamic-insert="false" dynamic-update="false">
        <id name="seqid" type="integer">
            <column name="seqid"/>
            <generator class="identity"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20"/>
        </property>
        <property name="classid" type="string">
            <column name="classid" length="20"/>
        </property>
        <property name="classdesc" type="string">
            <column name="classdesc" length="100"/>
        </property>
        <property name="notes" type="string">
            <column name="notes" length="65535"/>
        </property>
        <property name="trnid" type="string">
            <column name="trnid" length="20"/>
        </property>
        <property name="roomid" type="string">
            <column name="roomid" length="20"/>
        </property>
        <property name="slots" type="integer">
            <column name="slots"/>
        </property>
        <property name="classfee" type="float">
            <column name="classfee" precision="12"/>
        </property>
        <property name="instid" type="string">
            <column name="instid" length="20"/>
        </property>
        <property name="startdate" type="date">
            <column name="startdate" length="10"/>
        </property>
        <property name="enddate" type="date">
            <column name="enddate" length="10"/>
        </property>
        <property name="datecreated" type="date">
            <column name="datecreated" length="10"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>