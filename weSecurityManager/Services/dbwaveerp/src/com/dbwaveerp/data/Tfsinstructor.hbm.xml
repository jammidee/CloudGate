<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Tfsinstructor" table="tfsinstructor" catalog="dbwaveerp" dynamic-insert="false" dynamic-update="false">
        <id name="seqid" type="integer">
            <column name="seqid"/>
            <generator class="identity"/>
        </id>
        <property name="entityid" type="string">
            <column name="entityid" length="20"/>
        </property>
        <property name="instid" type="string">
            <column name="instid" length="20"/>
        </property>
        <property name="fname" type="string">
            <column name="fname" length="50"/>
        </property>
        <property name="mname" type="string">
            <column name="mname" length="50"/>
        </property>
        <property name="lname" type="string">
            <column name="lname" length="50"/>
        </property>
        <property name="email" type="string">
            <column name="email" length="100"/>
        </property>
        <property name="contactno" type="string">
            <column name="contactno" length="50"/>
        </property>
        <property name="userid" type="string">
            <column name="userid" length="20"/>
        </property>
        <property name="address" type="string">
            <column name="address" length="65535"/>
        </property>
        <property name="notes" type="string">
            <column name="notes" length="65535"/>
        </property>
        <property name="startdate" type="date">
            <column name="startdate" length="10"/>
        </property>
        <property name="enddate" type="date">
            <column name="enddate" length="10"/>
        </property>
        <property name="datecreated" type="date">
            <column name="datecreated" length="10"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>