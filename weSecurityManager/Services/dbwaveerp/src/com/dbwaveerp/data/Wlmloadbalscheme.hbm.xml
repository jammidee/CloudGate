<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Wlmloadbalscheme" table="wlmloadbalscheme" dynamic-insert="false" dynamic-update="false">
        <id name="juid" type="string">
            <column name="juid" length="36"/>
            <generator class="assigned"/>
        </id>
        <property name="seqno" type="integer">
            <column name="seqno"/>
        </property>
        <property name="serverid" type="string">
            <column name="serverid" length="20"/>
        </property>
        <property name="servername" type="string">
            <column name="servername" length="200"/>
        </property>
        <property name="appid" type="string">
            <column name="appid" length="20"/>
        </property>
        <property name="portno" type="string">
            <column name="portno" length="20"/>
        </property>
        <property name="curseq" type="integer">
            <column name="curseq"/>
        </property>
        <property name="nextseq" type="integer">
            <column name="nextseq"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
        <property name="deleted" type="integer">
            <column name="deleted"/>
        </property>
    </class>
</hibernate-mapping>