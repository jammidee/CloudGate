<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Wtmbundylist" table="wtmbundylist" dynamic-insert="false" dynamic-update="false">
        <id name="juid" type="string">
            <column name="juid" length="36"/>
            <generator class="assigned"/>
        </id>
        <property name="deviceid" type="string">
            <column name="deviceid" length="20"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="50"/>
        </property>
        <property name="useinout" type="string">
            <column name="useinout" length="5"/>
        </property>
        <property name="locid" type="string">
            <column name="locid" length="20"/>
        </property>
        <property name="pid" type="string">
            <column name="pid" length="36"/>
        </property>
        <property name="sstatus" type="string">
            <column name="sstatus" length="20"/>
        </property>
    </class>
</hibernate-mapping>