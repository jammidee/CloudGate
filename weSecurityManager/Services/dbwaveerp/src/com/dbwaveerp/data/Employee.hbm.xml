<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Employee" table="employee" catalog="dbwaveerp" dynamic-insert="false" dynamic-update="false">
        <id name="empid" type="integer">
            <column name="empid"/>
            <generator class="identity"/>
        </id>
        <property name="empname" type="string">
            <column name="empname"/>
        </property>
        <property name="empmanager" type="integer">
            <column name="empmanager"/>
        </property>
    </class>
</hibernate-mapping>