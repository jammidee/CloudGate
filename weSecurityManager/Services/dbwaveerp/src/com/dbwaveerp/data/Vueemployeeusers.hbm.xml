<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbwaveerp.data.Vueemployeeusers" table="vueemployeeusers" dynamic-insert="false" dynamic-update="false">
        <composite-id name="id" class="com.dbwaveerp.data.VueemployeeusersId">
            <key-property name="userid" type="string">
                <column name="userid" length="20"/>
            </key-property>
            <key-property name="partyid" type="string">
                <column name="partyid" length="20"/>
            </key-property>
            <key-property name="badgeid" type="string">
                <column name="badgeid" length="20"/>
            </key-property>
            <key-property name="email" type="string">
                <column name="email" length="100"/>
            </key-property>
            <key-property name="divisionid" type="string">
                <column name="divisionid" length="20"/>
            </key-property>
            <key-property name="departmentid" type="string">
                <column name="departmentid" length="20"/>
            </key-property>
            <key-property name="sectionid" type="string">
                <column name="sectionid" length="20"/>
            </key-property>
            <key-property name="locationid" type="string">
                <column name="locationid" length="20"/>
            </key-property>
            <key-property name="restdayid" type="string">
                <column name="restdayid" length="20"/>
            </key-property>
            <key-property name="shiftid" type="string">
                <column name="shiftid" length="20"/>
            </key-property>
            <key-property name="managerid" type="string">
                <column name="managerid" length="20"/>
            </key-property>
            <key-property name="sstatus" type="string">
                <column name="sstatus" length="20"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>